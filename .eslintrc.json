{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "eol-last": "off",
        "jsx-quotes": ["error", "prefer-single"],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "object-curly-newline": ["error", { "multiline": true }],
        "operator-linebreak": ["error", "after"],
        "max-len": ["error", { "code": 150 }]
    }
}
